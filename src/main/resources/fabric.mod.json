{
  "schemaVersion": 1,
  "id": "cursedchests",
  "version": "${version}",
  "name": "CursedChests",
  "description": "Adds cursed chests to minecraft.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "homepage": "https://minecraft.curseforge.com/projects/cursed-chests",
