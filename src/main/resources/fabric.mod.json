{
  "schemaVersion": 1,
  "id": "expandedstorage",
  "version": "${version}",
  "name": "ExpandedStorage",
  "description": "Adds new storage blocks with varying capacities.",
  "authors": [
    "NinjaPhenix"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/expanded-storage",
    "sources": "https://github.com/NinjaPhenix/ExpandedStorage/tree/1.15-snapshot"
  },
  "license": "MIT",
  "icon": "assets/expandedstorage/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ninjaphenix.expandedstorage.ExpandedStorage"
    ],
    "client": [
      "ninjaphenix.expandedstorage.ExpandedStorageClient"
    ],
    "rei_plugins_v0": [
      "ninjaphenix.expandedstorage.client.ExpandedStorageREIPlugin"
    ]
  },
  "mixins": [
    {
      "config": "expandedstorage.mixins.client.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.6.0",
    "fabric": ">=0.3.2"
  },
  "recommends": {
    "roughlyenoughitems": ">=3.1-pre+build.4"
